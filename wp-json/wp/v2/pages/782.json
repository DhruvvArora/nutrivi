n      if (age < 1) {\r\n        result.value = \"400-1000 micrograms.\";\r\n      } else if (age >= 1 && age <= 70) {\r\n        if (gender == \"male\") {\r\n          result.value = \"600-800 micrograms.\";\r\n        } else if (gender == \"female\") {\r\n          result.value = \"600-800 micrograms.\";\r\n        }\r\n      } else if (age > 70) {\r\n        result.value = \"800-1000 micrograms.\";\r\n      }\r\n    }\r\n  <\/script>\r\n<\/body>\r\n\r\n<\/html>\r\n\t\t<\/div>\n\t\t\t\t<\/div>\n\t\t\t\t\t<\/div>\n\t\t<\/div>\n\t\t\t\t\t\t\t<\/div>\n\t\t<\/section>\n\t\t\t\t<section class=\"elementor-section elementor-top-section elementor-element elementor-element-7396ebb elementor-section-boxed elementor-section-height-default elementor-section-height-default\" data-id=\"7396ebb\" data-element_type=\"section\" data-settings=\"{&quot;background_background&quot;:&quot;classic&quot;}\">\n\t\t\t\t\t\t<div class=\"elementor-container elementor-column-gap-default\">\n\t\t\t\t\t<div class=\"elementor-column elementor-col-100 elementor-top-column elementor-element elementor-element-9727b85\" data-id=\"9727b85\" data-element_type=\"column\">\n\t\t\t<div class=\"elementor-widget-wrap elementor-element-populated\">\n\t\t\t\t\t\t\t\t<div class=\"elementor-element elementor-element-c5795d3 elementor-widget elementor-widget-html\" data-id=\"c5795d3\" data-element_type=\"widget\" data-widget_type=\"html.default\">\n\t\t\t\t<div class=\"elementor-widget-container\">\n\t\t\t<!DOCTYPE html>\r\n<html>\r\n<head>\r\n  <title>Vitamin K Calculator<\/title>\r\n<\/head>\r\n<body>\r\n  <h3>Vitamin K intake<\/h3>\r\n<label for=\"gender\">Gender:<\/label>\r\n  <select id=\"gender\" name=\"gender\">\r\n    <option value=\"\">Select gender<\/option>\r\n    <option value=\"male\">Male<\/option>\r\n    <option value=\"female\">Female<\/option>\r\n  <\/select>\r\n\r\n  <label for=\"age\">Age:<\/label>\r\n  <input type=\"number\" id=\"age\" name=\"age\">\r\n\r\n  <label for=\"weight\">Weight (in 